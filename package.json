{
  "name": "Lunar",
  "version": "1.0.0",
  "author": "limao",
  "description": "农历托盘小工具",
  "dependencies": {
    "captchapng": "0.0.1",
    "gd-bmp": "^1.2.4",
    "menubar": "*"
  },
  "devDependencies": {
    "electron-packager": "^5.1.0",
    "electron": "^1.8.4",
    "electron-builder": "^19.19.1"
  },
  "main": "dist/main.js",
  "scripts": {
    "build": "electron-packager ./dist Lunar --platform=darwin --arch=x64 --version=1.8.6",
    "start": "electron .",
    "pack": "electron-builder"
  },
  "build": {
    "productName": "Lunar",
    "appId": "org.simulatedgreg.electron-vue",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/**/*"
    ],
    "mac": {
      "icon": "dist/panda.png"
    },
    "win": {
      "icon": "dist/panda.png"
    },
    "linux": {
      "icon": "dist/panda.png"
    }
  }
}
